    <div class="lhst_left-side-bar lhst_slider_small_screen lhst_vertical-alignment-top">
        <div class="lhst_left_side_bar_land_content row justify-content-md-center">
            <div class="col lhst_map_region_icon_left_bar_col">
                <img id="selected_land_coat_of_arms" src="../img/map-regions-icons-middle-ages/@(Model.Land.LandId).png" class="lhst_map_region_icon_left_bar" alt="map region coat of arms" loading="lazy" />
            </div>
            <div class="col lhst_current_land_text_content lhst_map_region_icon_left_bar_col mt-2">             
                <p id="selected_country_name" class="lhst_current_land_name mb-0 lhst_leftSideBar_country_name">@Model.Country.Name</p>                    
                <p id="selected_land_name" class="lhst_current_land_name lhst_leftSideBar_land_name">@Model.Land.LandId</p>
            </div>
        </div>
        <div>
            <div class="row">
                <img src="../img/default-icon/farmer.png" class="lhst_info_region_icon_left_bar float-left ml-2" title="Population" alt="" loading="lazy" />
                <p id="population" class="float-left mt-2 mb-0 lhst_clr-wheat" data-title="Population" >@Model.Population</p>
            </div>
            <div class="row">
                <img src="../img/default-icon/players.png" class="lhst_info_region_icon_left_bar float-left ml-2" title="Lords" alt="" loading="lazy" />
                <p id="lordsCount" class="lhst_cursor_pointer lhst_clr-wheat float-left mt-2 mb-0" data-title="Lords" onclick="openResidentsCountryDialog()">@Model.LordsCount</p>
            </div>
        </div>

        <div>
            <p id="map_border_with" class="ml-2 mb-0 mt-2 mr-2 lhst_clr-wheat" style="font-weight: 400">Has borders with: @Model.BorderWith</p>
        </div>

        <div>
            <p class="lhst_clr-wheat mt-3 mb-0 ml-2">Actions</p>
            <div class="lhst_line mb-1"></div>
        </div>
        <div class="ml-2">
            <div class="d-flex mb-1">
                <input id="moveToBtn" type="submit" class="lhst_btn_moveToRegion" data-selectedland=@Model.Land.LandId.Replace(' ', '_') value="Move to" onclick="moveToLand()" />
                <img src="../img/interface-icons/map-icons/currentland.svg" class="lhst_info_region_icon_left_bar float-left ml-2" title="Move to" alt="" loading="lazy" />
            </div>
            <div class="d-flex mb-1">
                <input id="settleBtn" type="submit" class="lhst_btn_moveToRegion" data-selectedland=@Model.Land.LandId.Replace(' ', '_') value="Settle down" onclick="settleDown()" />
                <img src="../img/interface-icons/map-icons/residence.svg" class="lhst_info_region_icon_left_bar float-left ml-2" title="Settle down" alt="" loading="lazy" />
            </div>
            <div class="d-flex mb-1">
                <button id="startAnUprisingBtn" type="submit" class="lhst_btn_moveToRegion" data-selectedland=@Model.Land.LandId.Replace(' ', '_') onclick="startAnUprising()">
                    <p class="w-100 flex-wrap mb-1 font-weight-normal">Start an uprising (<img class="lhst_independent_land_description_money_icon" src="~/img/interface-icons/coins.png" /> <span class="lhst_clr-brown"> -300</span>)</p>
                </button>
                <img src="../img/interface-icons/map-icons/revolt.svg" class="lhst_info_region_icon_left_bar float-left ml-2" title="Start an uprising" alt="" loading="lazy" />
            </div>
            @*   <input id="startAnUprisingBtn" type="submit" class="lhst_btn_moveToRegion" data-selectedland=@Model.Land.LandId.Replace(' ', '_') value="Start an uprising" onclick="startAnUprising()" /> *@
        </div>
        <div>
            <p class="font-weight-bolder mb-0 mt-2 ml-2 lhst_clr-wheat">Включить текстуры</p>
            <label class="switch">
                <input type="checkbox" id="texture-toggle" onchange="toggleTextures()">
                <span class="lhst_checkbox"></span>
            </label>
        </div>

        <div>
            <p class="font-weight-bolder mb-0 mt-2 ml-2 lhst_clr-wheat">Включить гербы</p>
            <label class="switch">
                <input type="checkbox" id="heraldic-toggle" onchange="toggleHeraldic()">
                <span class="lhst_checkbox"></span>
            </label>
        </div>

    </div>
